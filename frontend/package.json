{
  "name": "evaka-frontend",
  "version": "1.0.0",
  "description": "Frontend for eVaka",
  "private": true,
  "license": "LGPL-2.1-or-later",
  "scripts": {
    "build": "yarn wsrun --serial --exclude-missing build",
    "lint": "yarn wsrun --exclude-missing lint",
    "test": "yarn wsrun --exclude-missing test"
  },
  "devDependencies": {
    "prettier": "^2.2.1",
    "wsrun": "^5.2.0"
  },
  "optionalDependencies": {
    "@fortawesome/pro-light-svg-icons": "file:./vendor/fortawesome/fortawesome-pro-light-svg-icons-5.14.0.tgz",
    "@fortawesome/pro-regular-svg-icons": "file:./vendor/fortawesome/fortawesome-pro-regular-svg-icons-5.14.0.tgz",
    "@fortawesome/pro-solid-svg-icons": "file:./vendor/fortawesome/fortawesome-pro-solid-svg-icons-5.14.0.tgz"
  },
  "engines": {
    "node": ">= 14.15.0"
  },
  "workspaces": [
    "packages/*",
    "e2e-test",
    "eslint-plugin"
  ],
  "prettier": {
    "bracketSpacing": true,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none"
  }
}
