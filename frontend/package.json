    "polished": "^4.1.0",
    "react": "^17.0.1",
    "react-autosize-textarea": "^7.0.0",
    "react-chartjs-2": "^2.11.1",
    "react-csv": "^2.0.1",
    "react-datalist-input": "^1.2.12",
    "react-datepicker": "^2.10.1",
    "react-day-picker": "7.4.8",
    "react-dom": "^17.0.1",
    "react-focus-lock": "2.5.0",
    "react-leaflet": "^3.1.0",
    "react-meta-tags": "0.7.4",
    "react-router-dom": "^5.1.2",
    "react-select": "^3.0.8",
    "react-spring": "^8.0.27",
    "react-tooltip": "^4.2.13",
    "seamless-scroll-polyfill": "1.2.3",
    "styled-components": "5.2.1",
    "tslib": "^2.1.0"
  },
  "devDependencies": {
    "@sentry/webpack-plugin": "^1.14.0",
    "@storybook/addon-actions": "^6.1.17",
    "@storybook/react": "^6.1.17",
    "@types/classnames": "^2.2.11",
    "@types/jest": "^24.0.23",
    "@types/leaflet": "^1.5.21",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.31",
    "@types/react": "^17.0.1",
    "@types/react-csv": "^1.1.1",
    "@types/react-datepicker": "2.11.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-select": "3.0.22",
    "@types/react-tooltip": "^3.11.0",
    "@types/styled-components": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "babel-loader": "8.2.2",
    "bulma": "0.7.5",
    "css-loader": "^5.0.2",
    "csstype": "^3.0.6",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "express": "^4.17.1",
    "express-http-proxy": "^1.6.2",
    "fork-ts-checker-webpack-plugin": "^6.1.0",
    "html-webpack-plugin": "^5.0.0",
    "husky": "^5.0.9",
    "jest": "^24.9.0",
    "jest-junit": "^10.0.0",
    "make-error-cause": "^2.3.0",
    "parcel-bundler": "^1.12.4",
    "postcss-loader": "^5.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "sass": "^1.32.6",
    "style-loader": "^2.0.0",
    "testcafe": "~1.9.4",
    "testcafe-reporter-junit": "3.0.2",
    "ts-jest": "^24.2.0",
    "ts-loader": "^8.0.16",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^4.1.3",
    "webpack": "^5.21.2",
    "webpack-cli": "^4.5.0",
    "webpack-dev-middleware": "^4.1.0",
    "webpack-pwa-manifest": "^4.3.0"
  },
  "peerDependencies": {
    "@fortawesome/pro-light-svg-icons": "^5.14.0",
    "@fortawesome/pro-regular-svg-icons": "^5.14.0",
    "@fortawesome/pro-solid-svg-icons": "^5.14.0"
  },
  "peerDependenciesMeta": {
    "@fortawesome/pro-light-svg-icons": {
      "optional": true
    },
    "@fortawesome/pro-regular-svg-icons": {
      "optional": true
    },
    "@fortawesome/pro-solid-svg-icons": {
      "optional": true
    }
  },
  "engines": {
    "node": ">= 14.15.0"
  },
  "browserslist": [
    "Firefox ESR"
  ],
  "prettier": {
    "bracketSpacing": true,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none"
  },
  "postcss": {
    "plugins": {
      "postcss-preset-env": true
    }
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:@typescript-eslint/recommended-requiring-type-checking",
      "prettier/@typescript-eslint",
      "plugin:prettier/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "project": "./tsconfig.eslint.json"
    },
    "plugins": [
      "@typescript-eslint",
      "react-hooks"
    ],
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "rules": {
      "@typescript-eslint/explicit-function-return-type": "off",
      "@typescript-eslint/explicit-module-boundary-types": "off",
      "@typescript-eslint/member-delimiter-style": [
        "warn",
        {
          "multiline": {
            "delimiter": "none"
          },
          "singleline": {
            "delimiter": "semi",
            "requireLast": false
          }
        }
      ],
      "@typescript-eslint/no-unused-vars": [
        "warn",
        {
          "argsIgnorePattern": "^_",
          "varsIgnorePattern": "^_"
        }
      ],
      "react/prop-types": 0,
      "react-hooks/rules-of-hooks": "error"
    }
  }
}
