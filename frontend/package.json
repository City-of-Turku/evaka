{
  "name": "evaka-frontend",
  "version": "1.0.0",
  "description": "Frontend for eVaka",
  "private": true,
  "license": "LGPL-2.1-or-later",
  "scripts": {
    "clean": "rm -rf dist/",
    "lint": "eslint --ext .ts,.tsx src",
    "lint:fix": "yarn lint --fix"
  },
  "dependencies": {
    "@evaka/citizen-frontend": "link:src/citizen-frontend",
    "@evaka/employee-frontend": "link:src/employee-frontend",
    "@evaka/lib-common": "link:src/lib-common",
